<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,Log4net" />
  </configSections>
  <system.net>
    <defaultProxy useDefaultCredentials="true" />
  </system.net>
  <log4net>
    <appender name="CustomizedFileAppender" type="Dell.Services.SupportAssist.Logger.CustomizedFileAppender, Logger">
      <file value="C:\Windows\Temp\SupportAssistAgent\UILogs\SupportAssistUI.log" />
      <appendToFile value="true" />
      <rollingStyle value="Size" />
      <datePattern value="yyyyMMdd" />
      <maxSizeRollBackups value="10" />
      <maximumFileSize value="1MB" />
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%property{version}] [%property{pid} - %thread] %level %logger - %message%newline%exception" />
      </layout>
      <filter type="log4net.Filter.LevelRangeFilter">
        <param name="LevelMin" value="INFO" />
        <param name="LevelMax" value="ERROR" />
      </filter>
    </appender>
    <root>
      <level value="ALL" />
      <appender-ref ref="CustomizedFileAppender" />
    </root>
    <logger name="NHibernate" additivity="false">
      <level value="WARN" />
      <appender-ref ref="CustomizedFileAppender" />
    </logger>
  </log4net>
  <appSettings>
    <add key="LoginRedirect" value="http://{0}:{1}/App/Win10/index.html#/termsInternetCheck/:" />
    <add key="PreferencesRedirectURL" value="http://{0}:{1}/App/Win10/index.html#/views/preferences" />
    <add key="IndexPageURL" value="http://{0}:{1}/App/Win10/index.html" />
    <add key="Win10SIndexPageURL" value="http://{0}:{1}/App/Win10S/index.html" />
    <add key="WebSocketURL" value="wss://127.0.0.1:{0}/" />
    <add key="RegisterURL" value="http://{0}:{1}/App/Win10/index.html#/register" />
    <add key="MyAccountURL" value="http://{0}:{1}/App/Win10/index.html#/myAccount" />
    <add key="AccountDetailsUrl" value="http://{0}:{1}/App/Win10/index.html#/views/contactInfo" />
    <add key="RegistrationUrl" value="http://{0}:{1}/App/Win10/index.html#/termsInternetCheck" />
    <add key="ErrorPageUrl" value="\Content\applicationErrorView.html" />
    <add key="AboutSupportAssistURL" value="http://{0}:{1}/App/Win10/index.html#/views/aboutSA" />
    <add key="IsSRERunning" value="false" />
  </appSettings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="log4net" publicKeyToken="669e0ddf0bb1aa2a" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-2.0.8.0" newVersion="2.0.8.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-12.0.0.0" newVersion="12.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>